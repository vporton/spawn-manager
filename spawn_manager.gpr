with "ahven";
with "spawn_common";

project Spawn_Manager is

   for Main use ("spawn_manager");
   for Source_Dirs use ("src", "zeromq-Ada/src");
   for Object_Dir use "obj";

   Compiler_Switches := Spawn_Common.Compiler_Switches & "-gnata";

   package Compiler is
      for Default_Switches ("ada") use Compiler_Switches;
   end Compiler;

end Spawn_Manager;
